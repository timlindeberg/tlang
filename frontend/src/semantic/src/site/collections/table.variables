/*******************************
    User Variable Overrides
*******************************/


@_hoverColor: fade(@primaryColor, 10%);
@background: fade(@primaryColor, 5%);
@activeBackgroundHover: @_hoverColor;
@positiveBackgroundHover: @_hoverColor;
@negativeBackgroundHover: @_hoverColor;
@selectableBackground: @_hoverColor;
@headerBackground: white;
@stripedBackground: white;
@headerColor: @primaryColor;
@rowBorder: @border;
@cellBorder: @border;
