{
  "name": "tlang",
  "version": "1.0.0",
  "publisher": "Tim Lindeberg",
  "engines": {
    "vscode": "^1.5.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "tlang",
        "extensions": [".t"],
        "configuration": "language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tlang",
        "scopeName": "source.t",
        "path": "syntaxes/tlang.json"
      }
    ]
  }
}