// 2

object Main {
  def main() : Unit = {
    println(new H().run());
  }
}

class H {
  def run(): String = {
    var a: A;
    var c: C;
    a = new A();
    println(a.foo(0, "yo"));      
    a = new B();
    println(a.foo(0, "yo"));      
    c = new C();
    println(c.foo(0, "yo"));      // Error
    c = new D();
    println(c.foo(0, "yo"));      // Error   
    return "!";
  }
}

class A { 
  def foo(a: Int, b: String):Int = { return 0; }
}

class B extends A {
  def foo(a: Int, b: String): Int = { return 1; }
}

class C { 
  def foo():Int = { return 0; }
}

class D extends C {
  def foo(): Int = { return 1; }
}
