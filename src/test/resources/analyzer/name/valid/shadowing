object Main {
    def main() : Unit = {
        println(new Test().test(2));
        println(new Test2().test(2));
    }
}

class Test {
    var t: Int;
    var b: Test2;
    var c: Int[];
    def test(num : Int) : Test2 = {
        var b: Test2;
        c = new Int[5];
        t = 1;
        return c;
    }
}


class Test2 {
    var t: Int;
    def test(t : Int) : Int = {
        return t;
    }
}
