class Sorter[T] {

	def sort(a: List[T]): List[T] = {
		var i: Int;
		var j: Int;
		var N: Int;
		var q: Int;
		i = 1;
		N  = a.size() - 1;
		while(i < N){
			j = i;
			while(0 < j && 0 < a.get(j-1).compareTo(a.get(j))){
				q = this.swap(a, j-1, j);
				j = j - 1;
			}
			i = i + 1;
		}
		return a;
	}

	def swap(A: List[T], i: Int, j: Int): Int = {
		var tmp: T;
		var q: Int;
		tmp = A.get(i);
		q = A.set(i, A.get(j));
		q = a.set(j, tmp);
		return 0;
	}
}

class IntWrapper {
	var i: Int;

	def comapareTo(other: IntWrapper): Int = {
		return i - other.get();
	}

	def set(iarg: Int): Int = {
		i = iarg;
		return 0;
	}

	def get(): Int = {
		return i;
	}
}

class StringWrapper {
	var s: String;

	def comapareTo(other: StringWrapper): Int = {
		return true;
	}

	def set(sarg: String): Int = {
		s = sarg;
		return 0;
	}

	def get(): String = {
		return s;
	}
}
