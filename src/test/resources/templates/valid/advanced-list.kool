class Integer {
	var i: Int;

	def Integer(iarg: Int) = {
		i = iarg;
	}

	def set(iarg: Int): Int = {
		i = iarg;
		return 0;
	}

	def get(): Int = {
		return i;
	}

	def toString(): String = {
		return "" + i;
	}

	def compareTo(other: Integer): Int = {
		return i - other.get();
	}

	def equals(other: Integer): Bool = {
		return i == other.get();
	}
}

