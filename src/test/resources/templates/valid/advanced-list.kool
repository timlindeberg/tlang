class ListPrinter[T, U] {

	var l1: List[T];
	var l2: List[U];

	def ListPrinter(list1: List[T], list2: List[U]) = {
		l1 = list1;
		l2 = list2;
	}

	def print(msg: String): Int = {
		println("----------------------------------------");
		println("-- " + msg);
		println("----------------------------------------");
		println(l1);
		println(l2);
		return 0;
	}
}

