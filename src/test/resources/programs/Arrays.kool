object Main {
    def main() : Unit = {
      new H().run();
    }
}

class H {

  Def run(): Unit = {
     var i: Int;
     var int: Int[];
     var str: String[];
     var bool: Bool[];
     var a: A[];

     int = new Int[5];
     str = new String[5];
     bool = new Bool[5];
     a = new A[5];

     println(int.length); // 5
     println(str.length); // 5
     println(bool.length); // 5
     println(a.length); // 5

     for(i = 0; i < 5; i++){
        int[i]  = i;
        str[i]  = "" + i;
        bool[i] = i % 2 == 0;
        a[i]    = new A(i);
     }

     for(i = 0; i < 5; i++){
        print(int[i] + " "); // 0 1 2 3 4
     }
     println("");
     for(i = 0; i < 5; i++){
        print(str[i] + " "); // 0 1 2 3 4
     }
     println("");
     for(i = 0; i < 5; i++){
        print(bool[i]); // true false true false true
        print(" ");
     }
     println("");
     for(i = 0; i < 5; i++){
        print(a[i]);    // 0 1 2 3 4
        print(" ");
     }
  }

}

class A {

  var int: Int;

  Def A(i: Int) = {
    int = i;
  }

  Def toString(): String = {
    return "" + int;
  }

}
