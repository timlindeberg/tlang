var i: Int = 1
println(i) // res: 1
{
	var i: Int = 2
	println(i) // res: 2
	i += 5
	println(i) // res: 7
	if(i == 7) {
		var i: Int = 10
		println(i + 1) // res: 11
	}
	println(i) // res: 7
}
println(i) // res: 1
for(var i: Int = 0; i < 5; i ++) print(i + " ") // res: 0 1 2 3 4
println("")

println(i) // res: 1

var j: Int = 1
for(var i: Int = 0, var j: Int = 0; i + j < 10; i++, j++) print((i + j) + " ") // res: 0 2 4 6 8
println("")

println(i) // res: 1
println(j) // res: 1

Test(5)

Def Test(i: Int): Unit = {
	println(i) // res: 5
	{
		var i: Int = 1
		println(i) // res: 1
	}
	println(i) // res: 5
	var i: Int = 1
	println(i) // res: 1
}
