main Main = {
	new H().run();
}

class H {
  Def run(): Unit = {
    var i: Int;
    var j: Int;
    i = 0;
    j = 0;
    j = i++;
    println(i); // res: 1
    println(j); // res: 0
    j = i--;
    println(i); // res: 0
    println(j); // res: 1
    j = --i;
    println(i); // res: -1
    println(j); // res: -1
    j = ++i;
    println(i); // res: 0
    println(j); // res: 0
    i++;
    println(i); // res: 1
    i = -i;
    println(i); // res: -1
  }

}