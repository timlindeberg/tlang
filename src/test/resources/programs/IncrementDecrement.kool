main Main = {
	new H().run();
}

class H {
  
  Var M: Int = 0;
  Var N: Int = 0;

  Def run(): Unit = {
    var i: Int = 0;
    var j: Int = 0;
    var k: Float = 0.0f;
    var l: Float = 0.0f;

    j = i++;
    println(i); // res: 1
    println(j); // res: 0
    j = i--;
    println(i); // res: 0
    println(j); // res: 1
    j = --i;
    println(i); // res: -1
    println(j); // res: -1
    j = ++i;
    println(i); // res: 0
    println(j); // res: 0
    i++;
    println(i); // res: 1

    l = k++;
    println(k); // res: 1.0
    println(l); // res: 0.0
    l = k--;
    println(k); // res: 0.0
    println(l); // res: 1.0
    l = --k;
    println(k); // res: -1.0
    println(l); // res: -1.0
    l = ++k;
    println(k); // res: 0.0
    println(l); // res: 0.0
    k++;
    println(k); // res: 1.0

    M = N++;
    println(N); // res: 1
    println(M); // res: 0
    M = N--;
    println(N); // res: 0
    println(M); // res: 1
    M = --N;
    println(N); // res: -1
    println(M); // res: -1
    M = ++N;
    println(N); // res: 0
    println(M); // res: 0
    N++;
    println(N); // res: 1
  }

}