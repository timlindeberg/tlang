object Main {
    def main() : Unit = {
      new H().run();
    }
}

class H {
  def run(): Unit = {
    var i: Int;
    var j: Int;
    i = 0;
    j = 0;
    j = i++;
    println(i); // 1
    println(j); // 0
    j = i--;
    println(i); // 0
    println(j); // 1
    j = --i;
    println(i); // -1
    println(j); // -1
    j = ++i;
    println(i); // 0
    println(j); // 0
    i++;
    println(i); // 1
    i = -i;
    println(i); // -1
  }

}