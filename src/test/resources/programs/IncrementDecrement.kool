main Main = {
	new H().run();
}

class H {
  Def run(): Unit = {
    var i: Int = 0;
    var j: Int = 0;
    var k: Float = 0.0f;
    var l: Float = 0.0f;

    j = i++;
    println(i); // res: 1
    println(j); // res: 0
    j = i--;
    println(i); // res: 0
    println(j); // res: 1
    j = --i;
    println(i); // res: -1
    println(j); // res: -1
    j = ++i;
    println(i); // res: 0
    println(j); // res: 0
    i++;
    println(i); // res: 1

    l = k++;
    println(k); // res: 1.0
    println(l); // res: 0.0
    l = k--;
    println(k); // res: 0.0
    println(l); // res: 1.0
    l = --k;
    println(k); // res: -1.0
    println(l); // res: -1.0
    l = ++k;
    println(k); // res: 0.0
    println(l); // res: 0.0
    k++;
    println(k); // res: 1.0
  }

}