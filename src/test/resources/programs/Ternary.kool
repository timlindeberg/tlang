var i: Int = 5;
println(i == 5 ? 0 : 1);  // res: 0
println(true ? 0 : 1);    // res: 0
println(false ? 0 : 1);   // res: 1
println(Test(i) ? 0 : 1);  // res: 1
println(Test(10) || Test(9) || Test(8) || 7 > 8 || 8*5 + 3 - 40 < 3 ? 50*3 - 150 : 8 + 8 + 8 + (25*4) - 123);  // res: 1

Def Test(i: Int): Bool = {
    return i < 5;
}
