object Main {
    def main() : Unit = {
      new H().run();
    }
}

class H {

  Def run(): Unit = {
     var i: Int;
     i = 1;
     println(i << 0); // 1
     println(i << 1); // 2
     println(i << 2); // 4
     println(i << 3); // 8
     println(i << 4); // 16
     println(i << 5); // 32
     i = i << 5;

     println(i >> 0); // 32
     println(i >> 1); // 16
     println(i >> 2); // 8
     println(i >> 3); // 4
     println(i >> 4); // 2
     println(i >> 5); // 1

  }

}
