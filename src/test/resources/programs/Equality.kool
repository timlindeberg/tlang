object Main {
    def main() : Unit = {
        println(new Test().test("Hej"));
    }
}

class Test {

    Def test(s1 : String): String = {
        var num: Int;
        var a: A;
        var b: A;
        var c: A;
        var s2: String;
        var i1: Int[];
        var i2: Int[];

        num = 1;

        a = new A();
        b = new B();
        c = new A();
        s2 = "Hej";

        i1 = new Int[2];
        i1[0] = 0;
        i1[1] = 1;
        i2 = new Int[2];
        i2[0] = 0;
        i2[1] = 1;

        // primitives
        println(num == 1);      // true
        println(1 == 2);        // false
        println(true == true);  // true
        println(false == true); // false

        // strings
        println("Hej" == "Hej");// true, comparing string constants
        println(s1 == s2);      // true, both are constants
        s1 = s1 + "hej";
        s2 = s1 + "hej";
        println(s1 == s2);      // false

        // objects
        println(a == b);        // false
        println(a == c);        // false
        a = b;
        println(a == b);        // true


        // arrays
        println(i1 == i2);      // false
        i1 = i2;
        println(i1 == i2);      // true

        return "";
    }
}

class A { }

class B extends A { }
