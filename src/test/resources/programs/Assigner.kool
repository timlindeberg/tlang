main Main = {
	new H().run();
}

class H {

  Def run(): String = {
    var f: Foo;
    var t: Test;
    var a: A;
    var c: C;
    f = new Foo();
    println(f.bar(10000)); 
    println(f.baz(1));     
    println(f.foo());    
    t = new Test();
    println(t.Method());     
    t = new Test4();
    println(t.Method());     

    a = new A();
    println(a.foo(0, "yo")); 
    a = new B();
    println(a.foo(0, "yo"));  
    c = new C();
    println(c.foo());  
    c = new D();
    println(c.foo());   
    return "!";
  }

}

class Foo {
  Def bar(a: Int): Int = {
    a = 5;
    return a;
  }
  Def baz(a: Int): Int = {
    return a;
  }

  Def foo():String = { return "foo"; }
}

class Test { Def Method () : String = { return "String" ; } }
class Test2 extends Test { }
class Test3 extends Test2 { }
class Test4 extends Test3 { Def Method () : String = { return "Test4" ; } }

class A { 
  Def foo(a: Int, b: String):Int = { return 0; }
}

class B extends A {
  Def foo(a: Int, b: String): Int = { return 1; }
}

class C { 
  Def foo():Int = { return 0; }
}

class D extends C {
  Def foo(): Int = { return 1; }
}
