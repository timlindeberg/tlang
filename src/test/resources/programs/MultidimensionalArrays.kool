main Main = {
	new H().run();
}

class H {

   var arr: Int[][] = new Int[2][2];

  Def run(): Unit = {
     var intArray: Int[][][][] = new Int[1][2][3][4];
     var i: Int;
     var j: Int;
     var k: Int;
     var l: Int;

     println(intArray.length); // res: 1
     println(intArray[0].length); // res: 2
     println(intArray[0][0].length); // res: 3
     println(intArray[0][0][0].length); // res: 4

     for(i = 0; i < intArray.length; i++ )
        for(j = 0; j < intArray[0].length; j++ )
            for(k = 0; k < intArray[0][0].length; k++ )
                for(l = 0; l < intArray[0][0][0].length; l++ )
                    intArray[i][j][k][l] = i + j + k + l;


     for(i = 0; i < intArray.length; i++ )
        for(j = 0; j < intArray[0].length; j++ )
            for(k = 0; k < intArray[0][0].length; k++ )
                for(l = 0; l < intArray[0][0][0].length; l++ )
                   print(intArray[i][j][k][l] + " "); // res: 0 1 2 3 1 2 3 4 2 3 4 5 1 2 3 4 2 3 4 5 3 4 5 6

     println("");

     intArray = new Int[2][3][4][5];
     println(intArray.length); // res: 2
     println(intArray[0].length); // res: 3
     println(intArray[0][0].length); // res: 4
     println(intArray[0][0][0].length); // res: 5

     intArray[0] = new Int[4][5][6];
     println(intArray.length); // res: 2
     println(intArray[0].length); // res: 4
     println(intArray[0][0].length); // res: 5
     println(intArray[0][0][0].length); // res: 6

     intArray[0][0] = new Int[6][7];
     println(intArray.length); // res: 2
     println(intArray[0].length); // res: 4
     println(intArray[0][0].length); // res: 6
     println(intArray[0][0][0].length); // res: 7

     intArray[0][0][0] = new Int[8];
     println(intArray.length); // res: 2
     println(intArray[0].length); // res: 4
     println(intArray[0][0].length); // res: 6
     println(intArray[0][0][0].length); // res: 8

     println(getArray().length); // res: 2
     println(getArray()[0].length); // res: 2

     println(getArray()[0][0]); // res: 0
     println(getArray()[0][1]); // res: 1
     println(getArray()[1][0]); // res: 2
     println(getArray()[1][1]); // res: 3

     println(getArray()[0][0] = 1); // res: 1

     println(getFieldArray()[0][0] = 5); // res: 5
     println(arr[0][0]); // res: 5
  }

   def getArray(): Int[][] = {
      var i: Int[][] = new Int[2][2];
      i[0][0] = 0; i[0][1] = 1;
      i[1][0] = 2; i[1][1] = 3;
      return i;
   }

   def getFieldArray(): Int[][] = {
      return arr;
   }

}
