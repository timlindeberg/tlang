main Main = {
	new H().run();
}

class H {

  Def run(): Unit = {
     var c: Char = 'b';
     var i: Int = 2;
     var l: Long = 2l;
     var f: Float = 2f;
     var d: Double = 2.0;

     // Char
     println(c % c); // 0

     // Int
     println(i % i); // 0
     println(i % l); // 0
     println(i % f); // 0.0
     println(i % d); // 0.0
     println(i % c); // 0

     // Long
     println(l % l); // 0
     println(l % f); // 0.0
     println(l % d); // 0.0
     println(l % c); // 1

     // Float
     println(f % f); // 0.0
     println(f % d); // 0.0
     println(f % c); // 1.0

     // Double
     println(d % d); // 0.0
     println(d % c); // 1.0
  }

}

class A {

   Def toString(): String = {
      return "A";
   }

 }
