var c: Char = 'a';
var i: Int = 2;
var l: Long = 2l;

// Char
println(c >> c);  // res: 48

// Int
println(i >> i);  // res: 0
println(i >> l);  // res: 0
println(i >> c);  // res: 1

// Long
println(l >> l);  // res: 0
println(l >> c);  // res: 0

