var c: Char = 'a';
var i: Int = 1;
var l: Long = 1l;
var f: Float = 1f;
var d: Double = 1.0;

// Char
println(c - c);  // res: 0

// Int
println(i - i);  // res: 0
println(i - l);  // res: 0
println(i - f);  // res: 0.0
println(i - d);  // res: 0.0
println(i - c);  // res: -96

// Long
println(l - l);  // res: 0
println(l - f);  // res: 0.0
println(l - d);  // res: 0.0
println(l - c);  // res: -96

// Float
println(f - f);  // res: 0.0
println(f - d);  // res: 0.0
println(f - c);  // res: -96.0

// Double
println(d - d);  // res: 0.0
println(d - c);  // res: -96.0

class A {
   Def toString(): String = return "A";
}
