var c: Char = 'b';
var i: Int = 2;
var l: Long = 2l;
var f: Float = 2f;
var d: Double = 2.0;

  // Char
println(c / c);  // res: 1

// Int
println(i / i);  // res: 1
println(i / l);  // res: 1
println(i / f);  // res: 1.0
println(i / d);  // res: 1.0
println(i / c);  // res: 0

// Long
println(l / l);  // res: 1
println(l / f);  // res: 1.0
println(l / d);  // res: 1.0
println(l / c);  // res: 0

// Float
println(f / f);  // res: 1.0
println(f / d);  // res: 1.0
println(c / f);  // res: 49.0

// Double
println(d / d);  // res: 1.0
println(c / d);  // res: 49.0

class A {
   Def toString(): String = return "A";
}
