val i: Int? = 5
val j: Int? = null

if(i){
    println("Not Null") // res: Not Null
}else{
    println("Null")
}

if(j){
    println("Not Null")
}else{
    println("Null") // res: Null
}

println(i && j) // res: false
println(i || j) // res: true

val b1: Bool = i && j
val b2: Bool = i || j
println(b1) // res: false
println(b2) // res: true
