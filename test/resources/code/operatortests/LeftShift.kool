var c: Char = 'a';
var i: Int = 2;
var l: Long = 2l;

// Char
println(c << c);  // res: 194

// Int
println(i << i);  // res: 8
println(i << l);  // res: 8
println(i << c);  // res: 4

// Long
println(l << l);  // res: 8
println(l << c);  // res: 17179869184

class A {
   Def ToString(): String = return "A";
}
