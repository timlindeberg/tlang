var b: Bool = true;
var c: Char = 'a';
var i: Int = 1;
var l: Long = 1l;
var f: Float = 1f;
var d: Double = 1.0;
var s: String = "str";
var a: A = new A();

// String
println(s + s);  // res: strstr
println(s + a);  // res: strA
println(s + i);  // res: str1
println(s + l);  // res: str1
println(s + f);  // res: str1.0
println(s + d);  // res: str1.0
println(s + c);  // res: stra
println(s + b);  // res: strtrue

// Char
println(c + c);  // res: 194

// Int
println(i + i);  // res: 2
println(i + l);  // res: 2
println(i + f);  // res: 2.0
println(i + d);  // res: 2.0
println(i + c);  // res: 98

// Long
println(l + l);  // res: 2
println(l + f);  // res: 2.0
println(l + d);  // res: 2.0
println(l + c);  // res: 98

// Float
println(f + f);  // res: 2.0
println(f + d);  // res: 2.0
println(f + c);  // res: 98.0

// Double
println(d + d);  // res: 2.0
println(d + c);  // res: 98.0

class A {
   Def toString(): String = return "A";
}
