// ignore
var a = new A()

println(a.lol()) // res: lol
// Uses D:s version
println(a.implemented(0)) // res: 3


class A : B, C, D {

    Def method(i: Int) = return i + 1

}

class B {

    Def lol() = println("lol")

}

trait C {

    Def method(i: Int): Int

    Def implemented(i: Int) = return method(i) + 1
}

trait D {

    Def method(i: Int): Int

    Def implemented(i: Int) = return method(i) + 2
}
